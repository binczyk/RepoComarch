function getName(){
	var name = null;
	name = prompt("Enter your name: ");
	if (name == null)
		getName();
	retun name;
}

function getSurname(){
	var surname = null;
	surname = prompt("Enter your surname: ");
	if (surname == null)
		getSurame();
	retun surname;
}

function getAge(){
	var age = null;
	age = prompt("Enter your surname: ");
	if (age == null)
		getAge();
	retun age;
}