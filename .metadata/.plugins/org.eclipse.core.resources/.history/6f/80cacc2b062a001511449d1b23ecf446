function getName(){
	var name = null;
	name = prompt("Enter your name: ");
	console.log(name);
	if (name == "")
		getName();
	retun name;
}

function getSurname(){
	var surname = null;
	surname = prompt("Enter your surname: ");
	console.log(surname);
	if (surname == "")
		getSurame();
	retun surname;
}

function getAge(){
	var age = null;
	age = prompt("Enter your surname: ");
	console.log(age);
	if (age == null)
		getAge();
	retun age;
}